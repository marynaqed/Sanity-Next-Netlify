    "@uniformdev/optimize-tracker-storage-indexeddb": "^11.0.0",
    "analytics": "^0.7.13",
    "next": "11.1.0",
    "next-sanity": "^0.4.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@sanity/types": "^2.15.0",
